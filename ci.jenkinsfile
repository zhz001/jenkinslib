#!groovy

pipeline {
    agent { 
        node { 
            label "master" //指定运行节点的标签或者名称 
            }
    }
    stages {
        stage('bulid') { //阶段名称
            steps { //步骤
                timeout(5){
                    script{ //填写运行代码
                        mavHome = tool "M2"
                        sh "${mavHome}/bin/mvn -v"
                        println('bulid')
                    }
                }
            }
        }
    }
}
